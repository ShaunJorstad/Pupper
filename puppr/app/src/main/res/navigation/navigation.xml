<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.puppr.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_dogFinder"
            app:destination="@id/dogFinder" />
    </fragment>
    <fragment
        android:id="@+id/dogFinder"
        android:name="com.example.puppr.DogFinder"
        android:label="fragment_dog_finder"
        tools:layout="@layout/fragment_dog_finder" />
</navigation>